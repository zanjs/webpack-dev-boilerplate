{
  "name": "webpack-dev-boilerplate",
  "version": "1.0.2",
  "description": "boilerplate",
  "main": "app/index.jsx",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --progress",
    "build": "./node_modules/.bin/webpack --progress --colors --config webpack.prod.config.js",
    "lint": "./node_modules/.bin/eslint app",
    "clean": "rm -rf build/*",
    "test": "./node_modules/.bin/mocha --recursive --compilers js:babel-core/register --require ./test/setup.js",
    "test:watch": "npm run test -- --watch"
  },
  "betterScripts": {
    "dev": {
      "command": "./node_modules/.bin/webpack-dev-server --progress",
      "env": {
        "BUILD_DEV": "development"
      }
    },
    "build": {
      "env": {
        "BUILD_PRERELEASE": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zanjs/webpack-dev-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "es6",
    "babel"
  ],
  "author": "zanjs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zanjs/webpack-dev-boilerplate/issues"
  },
  "homepage": "https://github.com/zanjs/webpack-dev-boilerplate#readme",
  "dependencies": {
    "better-npm-run": "0.1.1",
    "bootstrap": "^4.3.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-react": "^7.14.3",
    "expect": "^24.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.1.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mocha": "^6.2.0",
    "open-browser-webpack-plugin": "0.0.5",
    "react-hot-loader": "^4.12.12",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-dev-server": "^3.8.0"
  }
}
